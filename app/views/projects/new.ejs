<%- include('../_flash') %>
<form action="/projects" method="POST">
  <input type="hidden" name="_csrf" value="<%= csrfToken %>">
  <label>Key</label>
  <input type="text" name="key" pattern="^[A-Z]+$">
  <label>Name</label>
  <input type="text" name="name">
  <button type="submit" class="btn btn-success">Create</button>
</form>

<%- contentFor('title') %>
Add project
