<form action="/projects/<%= project.key %>" method="PUT">
  <input type="hidden" name="_csrf" value="<%= csrfToken %>">
  <label>Name</label>
  <input type="text" name="name" value="<%= project.name %>">
  <button type="submit" class="btn btn-success">Edit</button>
</form>

<%- contentFor('title') %>
Edit project
